{
    "name": "wcheng/kaltura-entries-to-xml",
    "description": "Download media entries from Kaltura service to XML so you can make a bulk update and import it back into the service.",
    "authors": [
        {
            "name": "Wayne Cheng",
            "email": "waynethecheng@gmail.com"
        }
    ],
    "keywords": ["kaltura"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "kaltura/api-client-library": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "satooshi/php-coveralls": "dev-master",
	"mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "wcheng\\KalturaEntriesToXML\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
